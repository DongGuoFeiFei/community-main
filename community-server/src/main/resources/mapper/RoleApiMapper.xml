<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.communityserver.mapper.RoleApiMapper">

    <insert id="insertRoleApi">
        INSERT INTO role_api (role_id, api_id, create_time)
        values
        <foreach collection="param.ids" item="apiId" separator=",">
            (#{param.id},#{apiId},NOW())
        </foreach>
    </insert>
</mapper>
